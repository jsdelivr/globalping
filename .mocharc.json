{
	"exit": true,
	"timeout": 5000,
	"check-leaks": true,
	"file": [
		"test/setup.ts"
	],
	"spec": [
		"test/**/*.test.ts"
	],
	"node-option": [
		"experimental-specifier-resolution=node",
		"loader=ts-node/esm",
		"loader=testdouble"
	],
	"globals": [
		"__extends",
		"__assign",
		"__rest",
		"__decorate",
		"__param",
		"__metadata",
		"__awaiter",
		"__generator",
		"__exportStar",
		"__createBinding",
		"__values",
		"__read",
		"__spread",
		"__spreadArrays",
		"__spreadArray",
		"__await",
		"__asyncGenerator",
		"__asyncDelegator",
		"__asyncValues",
		"__makeTemplateObject",
		"__importStar",
		"__importDefault",
		"__classPrivateFieldGet",
		"__classPrivateFieldSet"
	]
}
